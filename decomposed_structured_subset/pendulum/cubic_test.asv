x = sdpvar(1, 1);
rho = sdpvar(1,1);

f = x^3 - x;
V = x^2/2;
Vdot = x * f;

%L = 1;
degL = 2;
[sL,cL] = polynomial(x,2);


p = (x'*x)*(V - rho) - sL*Vdot;

cons = [sos(p); sos(sL)]; 
obj  = -rho;

opts = sdpsettings('solver', 'sedumi');

%sol = optimize(cons, obj, opts)
sol = solvesos(cons,-rho,opts,[rho; cL]);
